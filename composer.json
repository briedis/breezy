{
    "name": "briedis/breezy",
    "description": "Simple breezy.hr API wrapper",
    "type": "library",
    "authors": [
        {
            "name": "martins.briedis",
            "email": "mbriedis@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/var-dumper": "^3.2",
        "mockery/mockery": "^0.9.7"
    },
    "autoload": {
        "psr-4": {
            "Briedis\\Breezy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Briedis\\Breezy\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
