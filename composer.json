{
  "name": "briedis/breezy",
  "description": "Simple breezy.hr API wrapper",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "martins.briedis",
      "email": "mbriedis@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Briedis\\Breezy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Briedis\\Breezy\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
